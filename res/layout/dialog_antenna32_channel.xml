    <!-- 读卡器设置end -->

    <LinearLayout
        android:id="@+id/linearLayout_setAntenna"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true" >

        <Button
            android:id="@+id/button_read_antenna"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.76"
            android:onClick="on_button_read_antenna"
            android:text="@string/read_antenna"
            android:textColor="#000000"
            android:textSize="16sp" />

        <Button
            android:id="@+id/button_set_antenna"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.76"
            android:onClick="on_button_set_antenna"
            android:text="@string/set_antenna"
            android:textColor="#000000"
            android:textSize="16sp" />
